{
    "name": "avro/support-bundle",
    "type": "symfony-bundle",
    "description": "A simple Symfony2 support bundle",
    "keywords": ["support"],
    "homepage": "http://github.com/avro/SupportBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Joris de Wit",
            "email": "joris.w.dewit@gmail.com"
        }
    ],
    "repositories": [
        {
          "type": "vcs",
          "url": "git@github.com:jdewit/HTMLPurifierBundle.git"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "exercise/htmlpurifier-bundle": "dev-feature-form-types",
        "knplabs/knp-paginator-bundle": "*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": { "Avro\\SupportBundle": "" }
    },
    "target-dir": "Avro/SupportBundle"
}
